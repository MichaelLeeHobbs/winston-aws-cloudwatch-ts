{
  "name": "winston-aws-cloudwatch-ts",
  "version": "1.0.0",
  "description": "A TypeScript Winston transport for Amazon CloudWatch.",
  "keywords": [
    "amazon",
    "aws",
    "cloudwatch",
    "winston",
    "log",
    "logs",
    "logging",
    "cloud",
    "typescript"
  ],
  "author": "Tim De Pauw <hi@tmdpw.eu> (https://tmdpw.eu/)",
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "pnpm@9.15.0",
  "peerDependencies": {
    "winston": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.864.0",
    "bottleneck": "^1.16.0",
    "debug": "^3.1.0",
    "winston-transport": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/globals": "^30.2.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jest": "^30.2.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.6.2",
    "rimraf": "^2.6.1",
    "sinon": "^18.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && pnpm run build:types && pnpm run build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "test": "pnpm run test:format && pnpm run test:lint && pnpm run test:unit",
    "test:format": "prettier --check \"{src,tests}/**/*.{ts,js,json,md}\"",
    "test:lint": "eslint",
    "test:unit": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "pnpm run test:format && pnpm run test:lint && pnpm run test:cover",
    "format": "prettier --write \"{src,tests}/**/*.{ts,js,json,md}\"",
    "lint": "eslint --fix",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelLeeHobbs/winston-aws-cloudwatch-ts.git"
  },
  "bugs": "https://github.com/MichaelLeeHobbs/winston-aws-cloudwatch-ts/issues",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write",
    "*.{ts,js}": "eslint --fix"
  }
}
