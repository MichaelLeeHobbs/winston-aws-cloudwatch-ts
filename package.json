{
  "name": "@ubercode/winston-cloudwatch",
  "version": "1.0.0",
  "description": "A TypeScript Winston transport for Amazon CloudWatch.",
  "keywords": [
    "amazon",
    "aws",
    "cloudwatch",
    "winston",
    "log",
    "logs",
    "logging",
    "cloud",
    "typescript"
  ],
  "author": "Michael Lee Hobbs <michael.lee.hobbs@gmail.com> (https://github.com/MichaelLeeHobbs)",
  "contributors": [
    "Tim De Pauw <hi@tmdpw.eu> (https://tmdpw.eu/)"
  ],
  "engines": {
    "node": ">=20.9.0"
  },
  "packageManager": "pnpm@9.15.0",
  "peerDependencies": {
    "winston": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.864.0",
    "bottleneck": "^2.19.5",
    "debug": "^4.4.3",
    "winston-transport": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/globals": "^30.2.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jest": "^30.2.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "sinon": "^18.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "preversion": "pnpm run test && pnpm run build",
    "postversion": "git push && git push --tags",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "dry-run": "npm pack --dry-run",
    "test": "pnpm run test:format && pnpm run test:lint && pnpm run test:unit",
    "test:format": "prettier --check \"{src,tests}/**/*.{ts,js,json,md}\" \"*.{mts,ts,json}\" \".github/**/*.yml\"",
    "test:lint": "eslint",
    "test:unit": "node --max-old-space-size=6144 node_modules/jest/bin/jest.js",
    "test:cover": "node --max-old-space-size=6144 node_modules/jest/bin/jest.js --coverage",
    "test:watch": "node --max-old-space-size=6144 node_modules/jest/bin/jest.js --watch",
    "test:ci": "pnpm run test:format && pnpm run test:lint && pnpm run test:cover",
    "format": "prettier --write \"{src,tests}/**/*.{ts,js,json,md}\" \"*.{mts,ts,json}\" \".github/**/*.yml\"",
    "lint": "eslint --fix",
    "precommit": "lint-staged"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelLeeHobbs/winston-aws-cloudwatch-ts.git"
  },
  "homepage": "https://github.com/MichaelLeeHobbs/winston-aws-cloudwatch-ts#readme",
  "bugs": {
    "url": "https://github.com/MichaelLeeHobbs/winston-aws-cloudwatch-ts/issues"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write",
    "*.{ts,js}": "eslint --fix"
  }
}
